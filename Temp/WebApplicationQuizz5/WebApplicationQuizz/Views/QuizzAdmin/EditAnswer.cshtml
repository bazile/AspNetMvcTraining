@model WebApplicationQuizz.Models.EditAnswerModel
@{
    ViewBag.Title = "EditAnswer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Редактируем ответ @Model.Text</h2>

<form action="@Url.Action("EditAnswer")" method="post">
    @Html.Partial("answer", Model)
    <input type="submit" value="Сохранить" />
</form>
