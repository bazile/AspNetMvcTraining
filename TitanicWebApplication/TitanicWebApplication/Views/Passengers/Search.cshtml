@model IEnumerable<TitanicWebApplication.Db.TitanicDbPassenger>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (!Model.Any())
{
    <div>Nothing was found</div>
}
else
{
    <ul>
    @foreach (var pax in Model)
    {
        <li><a href="@Url.Action("Info", new { id = pax.UniqueId })">@pax.FullName</a></li>
    }
    </ul>
}
